<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.damnvulnerablebank">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".frates">
            <intent-filter>
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.DEFAULT" />
            <category android:name="android.intent.category.BROWSABLE" />

            <data
                android:host="frates.com"
                android:scheme="http" />
            <data
                android:host="frates.com"
                android:scheme="https" /></intent-filter></activity>
        <activity android:name=".banksecrets" />
        <activity android:name=".passcode" />
        <activity android:name=".confirmtrans">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="sendmoney"
                    android:scheme="dvba" />
                <data
                    android:host="sendmoney.dvba"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity android:name=".transfermoney" />
        <activity android:name=".forgetpassword" />
        <activity android:name=".requestpassword" />
        <activity android:name=".Dashboard" />
        <activity android:name=".signup" />
        <activity android:name=".login" />
        <activity android:name=".banklogin" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>